  "compilerOptions": {
    "target": "es6",
    "jsx": "react-jsx",
    "lib": ["dom", "es2019"],
    "allowSyntheticDefaultImports": true,
    "module": "ES2020",
    "moduleResolution": "Node",
    "typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  }
}
